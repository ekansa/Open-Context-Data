    <arch:observation obsNumber="1">
      <arch:properties>
        <arch:property>
          <arch:variableID>BE3C5D29-840E-4746-EF5D-1F8D3EB61B8C</arch:variableID>
          <arch:valueID>91F3B682-61F5-4BAA-97D9-8BBDCF14CD0C</arch:valueID>
          <oc:propid href="http://opencontext.org/properties/12FE0BF0-0E9C-4012-3F43-EEEC08028692">12FE0BF0-0E9C-4012-3F43-EEEC08028692</oc:propid>
