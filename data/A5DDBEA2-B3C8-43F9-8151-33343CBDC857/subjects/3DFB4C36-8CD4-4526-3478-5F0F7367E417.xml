          <oc:var_label type="alphanumeric">Abuts</oc:var_label>
          <oc:show_val>Locus 4, bedrock, to the south, and Locus 10, south wall, to the north.</oc:show_val>
        </arch:property>
        <arch:property>
          <arch:variableID>0C351ACF-AD2B-4D9E-CCE4-AAA50DA8A986</arch:variableID>
          <arch:valueID>F4AA42C9-EC41-492C-CF1D-ACC93F01DE80</arch:valueID>
