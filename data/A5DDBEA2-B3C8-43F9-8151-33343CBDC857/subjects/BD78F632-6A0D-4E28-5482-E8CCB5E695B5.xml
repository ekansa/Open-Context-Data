          <oc:var_label type="nominal">Stage Description</oc:var_label>
          <oc:show_val>Substructures and canalization for temple staircases and Lower Temenos Retaining Wall built.</oc:show_val>
        </arch:property>
        <arch:property>
          <arch:variableID>DDA9A808-6B43-45EF-4DB2-935C9C53E71F</arch:variableID>
