          <oc:var_label type="nominal">Stage Description</oc:var_label>
          <oc:show_val>Construction of Cryptoporticus Walls and Wall K.</oc:show_val>
        </arch:property>
        <arch:property>
          <arch:variableID>D3CBA50F-B4EF-4349-7DA7-AC07198EFA4B</arch:variableID>
          <arch:valueID>6737AE6A-7ACA-41D2-B9CA-FA71CCA079FB</arch:valueID>
