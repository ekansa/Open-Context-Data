          <oc:var_label type="nominal">Phase Description</oc:var_label>
          <oc:show_val>Phase XIV: Modern period marked by Bedouin activity and the accumulation of recent debris.</oc:show_val>
        </arch:property>
        <arch:property>
          <arch:variableID>0C351ACF-AD2B-4D9E-CCE4-AAA50DA8A986</arch:variableID>
