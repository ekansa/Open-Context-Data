    <oc:person_links>
      <oc:link href="http://opencontext.org/persons/8FC09711-CAD6-41EB-7B6E-B21B8133912A" cite="contributor">
        <oc:name>Elvan Cobb</oc:name>
        <oc:id>8FC09711-CAD6-41EB-7B6E-B21B8133912A</oc:id>
