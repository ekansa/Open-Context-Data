    </oc:diary_links>
    <oc:person_links>
      <oc:link href="http://opencontext.org/persons/9C80BF6B-7BDE-4295-E7D9-695845A44846" cite="contributor">
        <oc:name>Andrew Creekmore</oc:name>
        <oc:id>9C80BF6B-7BDE-4295-E7D9-695845A44846</oc:id>
        <oc:relation>Recorded by</oc:relation>
      </oc:link>
